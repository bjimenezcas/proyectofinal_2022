{
  "name": "bjimenezcas/proyectofinal_2022",
  "description": "Proyecto de boda",
  "keywords": [
    "yii2",
    "framework",
    "boda"
  ],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/bjimenezcas/proyectofinal_2022/issues?state=open",
    "source": "https://github.com/bjimenezcas/proyectofinal_2022"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=8.0.0",
    "yiisoft/yii2": "*",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-symfonymailer": "*",
    "kartik-v/yii2-widgets": "*",
    "kartik-v/yii2-datecontrol": "*",
    "kartik-v/yii2-detail-view": "*",
    "kartik-v/yii2-checkbox-x": "*",
    "kartik-v/yii2-widget-growl": "*",
    "kartik-v/yii2-date-range": "*",
    "kartik-v/yii2-dynagrid": "*",
    "yiisoft/yii2-bootstrap5": "*",
    "npm-asset/jquery": "*",
    "kartik-v/yii2-helpers": "*",
    "kartik-v/yii2-export": "dev-master",
    "kartik-v/yii2-grid": "*",
    "kartik-v/yii2-bootstrap5-dropdown": "@dev"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "*"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  }
}
